<link rel="stylesheet" href="/stylesheets/taxonomy.css">
{{> taxonomySidebar}}

<div class='my-container'>
    <div class='row'>
        {{#each cluster}}
            <div class='col-sm-3'>
                <div class="my-form" id="checkboxes">
                    <input class="form-check-input" type="checkbox" id="{{this}}" checked>
                    <img src="/obj_GIF/{{this}}.gif" alt="" width="100%" height="100%">
                
                </div>
                
            </div>
        {{/each}}

    </div>
</div>

<script src="/javascripts/taxonomy.js"></script>